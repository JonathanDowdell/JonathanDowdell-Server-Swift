# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

inhibit_all_warnings!

target 'Server Swift (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  inhibit_all_warnings!
  # Pods for Server Swift (iOS)

  pod 'SwiftSH', :inhibit_warnings => true
  
  pod 'SwiftyRSA'
  
  pod 'SwiftKeychainWrapper'

end

target 'Server Swift (macOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Server Swift (macOS)

end
